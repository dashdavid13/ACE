<%= form_for :session, url: login_path do |f| %>
   
      <%= f.label :username %>
      <%= f.text_field :username %>
<br>
        <%= f.label :password %>
        <%= f.text_field :password %>
   
    <%= f.submit "Log in"%>
    
    
  <% end %>
<br>
<br>
  <%= button_to "Sign up", new_user_path, method: :get %>

