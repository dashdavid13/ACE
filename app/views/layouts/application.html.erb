<!DOCTYPE html>
<html>
  <head>
    <title>ACE</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
   
    <div class="topnav">
     <%= yield %>
    <% if @current_user %>
    <%= link_to "Logout", logout_path, method: :delete  %></li>
    
   
    <%= link_to "Profile", user_path(@current_user)%>
    Welcome <%= @current_user.full_name %></li>
  <% else %>
    <%= link_to "Sign Up", new_user_path %></li>
    <%= link_to "Login", new_login_path %></li>
  <% end %>
  </div>



  </body>
</html>
